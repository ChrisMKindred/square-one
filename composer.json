{
    "name": "moderntribe/square-one",
        "type": "project",
        "description": "The starting point for Modern Tribe service works.",
        "license": "Private",
        "authors": [
        {
            "name": "Modern Tribe",
            "email": "peter@tri.be"
        }
    ],
        "minimum-stability": "dev",
        "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:moderntribe/glomar"
        },
        {
            "type": "package",
            "package": {
                "name": "wordpress",
                "type": "webroot",
                "version": "4.1",
                "dist": {
                    "type": "zip",
                    "url": "https://wordpress.org/latest.zip"
                },
                "require" : {
                    "fancyguy/webroot-installer": "1.0.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ],
        "require": {
            "wordpress": "4.1",
            "moderntribe/tribe-glomar": "~1.0",
            "wpackagist-plugin/posts-to-posts": "*",
            "wpackagist-plugin/limit-login-attempts": "*",
            "wpackagist-plugin/regenerate-thumbnails": "*",
            "wpackagist-plugin/wordpress-importer": "*",
            "badcow/lorem-ipsum": "dev-master",
            "fancyguy/webroot-installer": "1.0.0"
        },
        "config":{
            "vendor-dir": "dev/vendor"
        },
        "extra":{
            "installer-paths": {
                "content/plugins/{$name}/": ["type:wordpress-plugin"],
                "content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
                "wp/": ["type"]
            },
            "webroot-dir": "wp",
            "webroot-package": "wordpress"
        }
}
