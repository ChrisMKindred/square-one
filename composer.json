{
  "name": "moderntribe/square-one",
  "authors": [
    {
      "name": "Modern Tribe",
      "email": "admin@tri.be"
    }
  ],
  "description": "Modern Tribe's Square One WordPress setup",
  "type": "project",
  "license": "Proprietary",
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type":"vcs",
      "url":"git@github.com:moderntribe/attachment-helper.git"
    },
    {
      "type":"vcs",
      "url":"git@github.com:moderntribe/ajax-queue.git"
    },
    {
      "type":"vcs",
      "url":"git@github.com:moderntribe/panel-builder.git"
    },
    {
      "type":"vcs",
      "url":"git@github.com:humanmade/Mercator.git"
    }
  ],
  "require": {
    "composer/installers":"1.3.0",
    "johnpbloch/wordpress-core-installer":"1.0.0",
    "johnpbloch/wordpress-core": "4.7.5",
    "wpackagist-plugin/configure-smtp": "3.1",
    "wpackagist-plugin/gravity-forms-wcag-20-form-fields": "1.5.1",
    "wpackagist-plugin/limit-login-attempts": "2.0beta3",
    "wpackagist-plugin/posts-to-posts": "1.6.5",
    "wpackagist-plugin/regenerate-thumbnails": "2.2.6",
    "wpackagist-plugin/wordpress-seo": "4.1",
    "moderntribe/panel-builder": "dev-3.2-branch",
    "humanmade/mercator":"dev-master#671f147",
    "vlucas/phpdotenv": "^2.4"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp-content/mu-plugins/{$name}": [ "type:wordpress-muplugin"],
      "wp-content/plugins/{$name}":[
        "type:wordpress-plugin",
        "moderntribe/panel-builder"
      ]
    }
  }
}
