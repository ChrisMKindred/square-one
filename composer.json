{
    "name": "tribe/square-one",
    "type": "project",
    "description": "The starting point for Modern Tribe service works.",
    "license": "Private",
    "authors": [
        {
            "name": "Modern Tribe",
            "email": "peter@tri.be"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
    {
        "type": "vcs",
        "url": "git@github.com:moderntribe/glomar"
    },
    {
        "type": "composer",
        "url": "http://wpackagist.org"
    }
    ],
    "require": {
        "johnpbloch/wordpress": ">=4.1",
        "tribe/tribe-glomar": "~1.0",
        "wpackagist-plugin/posts-to-posts": "*",
        "badcow/lorem-ipsum": "dev-master"
    },
    "config":{
        "vendor-dir": "dev/vendor"
        },
    "extra":{
        "installer-paths": {
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/mu-plugins/{$name}/": ["type:wordpress-muplugin"]
        },
        "wordpress-install-dir": "wp"
    }
}
