{
  "name": "moderntribe/square-one",
  "authors": [
    {
      "name": "Modern Tribe",
      "email": "admin@tri.be"
    }
  ],
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": {
      "moderntribe/*": "source",
      "*": "dist"
    }
  },
  "description": "Modern Tribe's Square One WordPress setup",
  "type": "project",
  "license": "Proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type":"vcs",
      "url":"https://github.com/moderntribe/gutenpanels.git"
    }
  ],
  "require": {
    "php":"^7.2",
    "aws/aws-sdk-php": "3.48.4",
    "hirak/prestissimo":"0.3.7",
    "composer/installers":"1.4.0",
    "johnpbloch/wordpress-core-installer":"1.0.0",
    "johnpbloch/wordpress-core": "5.2.1",
    "wpackagist-plugin/configure-smtp": "3.1",
    "wpackagist-plugin/regenerate-thumbnails": "3.0.2",
    "moderntribe/gutenpanels": "dev-master",

    "moderntribe/square1-assets": "dev-feature/monorepo",
    "moderntribe/square1-cache": "dev-feature/monorepo",
    "moderntribe/square1-cli": "dev-feature/monorepo",
    "moderntribe/square1-container": "dev-feature/monorepo",
    "moderntribe/square1-nav": "dev-feature/monorepo",
    "moderntribe/square1-object-meta": "dev-feature/monorepo",
    "moderntribe/square1-post-type": "dev-feature/monorepo",
    "moderntribe/square1-taxonomy": "dev-feature/monorepo",

    "vlucas/phpdotenv": "^3.0",
    "humanmade/s3-uploads":"1.1.0",
    "pimple/pimple": "~3.0@dev",
    "johnbillion/extended-cpts": "^4.0",
    "twig/twig": "^2.11",
    "jbzoo/pimpledumper": "^1.2",
    "mailgun/mailgun-php": "^2.8",
    "php-http/curl-client": "^1.7",
    "php-http/client-common": "^1.9.0",
    "guzzlehttp/psr7": "^1.4",
    "sunra/php-simple-html-dom-parser": "1.5.2",
    "monolog/monolog":"1.23.0",
    "roots/wp-password-bcrypt": "dev-master"
  },
  "require-dev": {
    "wp-cli/wp-cli": "^2.2.0",
    "lucatume/wp-browser": "^2.2.14",
    "lucatume/function-mocker": "~1.0",
    "php-http/httplug": "^1.1",
    "filp/whoops": "^2.2@dev"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp-content/mu-plugins/{$name}": [ "type:wordpress-muplugin"],
      "wp-content/plugins/{$name}":[
        "type:wordpress-plugin",
        "moderntribe/gutenpanels"
      ],
      "wp-content/plugins/s3-uploads": [ "humanmade/S3-Uploads" ]
    }
  },
  "autoload": {
    "psr-4": {
      "Tribe\\Project\\":"wp-content/plugins/core/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tribe\\Tests\\": "dev/tests/tests/_support/Classes/"
    }
  }
}
