{
    "collectCoverage": false,
    "coverageDirectory": "<rootDir>/wp-content/themes/core/js/test/__coverage__",
    "coveragePathIgnorePatterns": [
        "/node_modules",
        "jest.setup.js"
    ],
    "coverageReporters": [
        "json",
        "lcov",
        "text-summary"
    ],
    "moduleNameMapper": {
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/wp-content/themes/core/js/test/__mocks__/fileMock.js",
        "\\.(css|pcss)$": "identity-obj-proxy"
    },
    "setupFiles": [
        "core-js/es7",
        "raf/polyfill",
        "<rootDir>/jest.setup.js"
    ],
    "snapshotSerializers": [
        "enzyme-to-json/serializer"
    ],
    "testRegex": ".*(test|spec)\\.jsx?$",
    "testEnvironment": "jest-environment-jsdom",
    "testPathIgnorePatterns": [
        "/node_modules/"
    ]
} 
