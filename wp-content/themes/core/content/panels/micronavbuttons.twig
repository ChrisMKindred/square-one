{# Panel: Micro Navigation Buttons #}

{% extends "content/panels/panel.twig" %}

{% block content %}

	<div class="s-content">
		<div class="g-row g-row--center g-row--col-4"
		     data-depth="0"
			 data-name="items"
			 data-livetext>

				{% for item in items %}
					<div class="g-col g-col--horizontal-center">
						{{ item }}
					</div>
				{% endfor %}

			</div>
		</div>
	</div>

{% endblock %}
