:root {

	--jquery-datepicker-bgd-color: var(--primary);
	--jquery-datepicker-bgd-color-secondary: #6b6b6b;
	--jquery-datepicker-bgd-color-tertiary: var(--secondary);
	--jquery-datepicker-bgd-color-quaternary: #6b6b6b;
	--jquery-datepicker-bgd-color-active: lighten(#6b6b6b, .12);

	--jquery-datepicker-color: var(--white);
	--jquery-datepicker-color-option: var(--text);
	--jquery-datepicker-color-disabled: rgba(255, 255, 255, .65);

}
